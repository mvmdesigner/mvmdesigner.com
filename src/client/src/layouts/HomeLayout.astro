---
import BaseLayout from '@layouts/BaseLayout.astro'

export interface Props {
	title: string;
	contentType: string;
	description: string;
	image: string;
}
const { 
	title,
	contentType = "website",
    description,
    image
 } = Astro.props as Props;
---

<BaseLayout {title} {contentType} {description} {image}>
	<!-- <Navbar client:load></Navbar> -->
	<main class="flex-1 flex flex-col">
		<slot />
	</main>
	<!-- <NavFooter></NavFooter> -->
</BaseLayout>